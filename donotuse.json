{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        //"plugin:prettier/recommended",
        "plugin:react/recommended",
        "airbnb",
        "react-app"
    ],
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },  
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "linebreak-style": 0
    }
}
